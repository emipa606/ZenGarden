<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>[SYR] Blueberries</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/RecipeDef[defName="SRV_MakeVinegarJuice"]/ingredients/li/filter/thingDefs</xpath>
          <value>
            <li>RawBlueberries</li>
          </value>
          <order>Append</order>
        </li>
        <li Class="PatchOperationAdd">
          <success>Always</success>
          <xpath>/Defs/RecipeDef[defName="SRV_MakeVinegarJuice"]/fixedIngredientFilter/thingDefs</xpath>
          <value>
            <li>RawBlueberries</li>
          </value>
          <order>Append</order>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>
