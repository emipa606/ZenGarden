<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ResourceVerbBase">
    <defName>ZEN_CherryWood</defName>
    <label>cherry wood</label>
    <description>Wood from cherry trees. Useful for building structures, weapons, and even simple prosthetics. A raw stick of cherrywood can be used as a weapon in a pinch.</description>
    <graphicData>
      <texPath>Cupro/Item/Material/CherryWood</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.6</MarketValue>
      <Mass>0.4</Mass>
      <MaxHitPoints>150</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
      <SharpDamageMultiplier>0.3</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.8</BluntDamageMultiplier>
    </statBases>
    <stuffProps>
      <stuffAdjective>cherrywood</stuffAdjective>
      <categories>
        <li>Woody</li>
      </categories>
      <commonality>0.35</commonality>
      <color>(211,110,84)</color>
      <constructEffect>ConstructWood</constructEffect>
      <appearance>Planks</appearance>
      <soundImpactBullet>BulletImpact_Wood</soundImpactBullet>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
      <statOffsets>
        <Beauty>2</Beauty>
      </statOffsets>
      <statFactors>
        <MaxHitPoints>0.6</MaxHitPoints>
        <Beauty>1.4</Beauty>
        <Flammability>1.0</Flammability>
        <WorkToMake>0.75</WorkToMake>
        <WorkToBuild>0.7</WorkToBuild>
        <DoorOpenSpeed>1.2</DoorOpenSpeed>
        <MeleeWeapon_CooldownMultiplier>0.85</MeleeWeapon_CooldownMultiplier>
      </statFactors>
    </stuffProps>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <tools>
      <li>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2.2</cooldownTime>
      </li>
    </tools>
  </ThingDef>
